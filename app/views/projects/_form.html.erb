<%= simple_form_for @profile do |f| %>
  <%= f.fields_for :projects do |builder| %>
    <%= builder.input :title, label: "Title" %>
    <%= builder.input :description, label: "Description" %>
    <%= builder.input :weblink, label: "Web Link" %>
    <%= builder.input :sourcecode, label: "Source Code" %>
    <%= builder.input :logo_project, label: "Project Logo" %>
    <%= builder.submit "Submit", class: "btn btn-info" %>
  <% end %>
<% end %>