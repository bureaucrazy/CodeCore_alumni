<%= simple_form_for [@profile, @project] do |f| %>
  <%= f.input :title, label: "Title" %>
  <%= f.input :description, label: "Description" %>
  <%= f.input :weblink, label: "Web Link" %>
  <%= f.input :sourcecode, label: "Source Code" %>
  <%= f.input :logo_project, label: "Project Logo" %>
  <%= f.submit "Submit", class: "btn btn-info" %>
<% end %>
