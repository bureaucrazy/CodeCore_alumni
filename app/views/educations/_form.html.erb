<%= simple_form_for @profile do |f| %>
  <%= f.fields_for :educations do |builder| %>
    <%= builder.input :school_name, label: "School", placeholder: 'e.g. CodeCore' %>
    <%= builder.input :year_from, label: "Year Start" %>
    <%= builder.input :year_to, label: "Year End" %>
    <%= builder.input :degree, label: "Degree Held", placeholder: 'e.g. Computer Science' %>
    <%= builder.input :logo_education, label: "School Logo" %>
    <%= builder.submit "Submit", class: "btn btn-info" %>
  <% end %>
<% end %>